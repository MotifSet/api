        "@0xproject/types": "^1.0.1",
        "@0xproject/utils": "^1.0.8",
        "@0xproject/web3-wrapper": "^2.0.2",
        "@types/superagent": "^3.8.4",
        "augur.js": "^5.6.0",
        "body-parser": "^1.17.1",
        "concurrently": "^4.0.1",
        "express": "^4.15.2",
        "express-async-wrap": "^1.0.0",
        "lodash": "^4.17.5",
        "request": "^2.88.0",
        "superagent": "^4.0.0-beta.5",
        "ts-node": "^7.0.1",
        "ts-node-dev": "^1.0.0-pre.30",
