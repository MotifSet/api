        "awesome-typescript-loader": "^3.1.3",
        "dotenv-cli": "^1.4.0",
        "gulp": "^3.9.1",
        "make-promises-safe": "^1.1.0",
        "nodemon": "^1.11.0",
        "prettier": "^1.14.2",
        "shx": "^0.2.2",
        "source-map-loader": "^0.1.6",
        "tslint": "5.11.0",
        "typescript": "3.0.1",
        "webpack": "^3.1.0",
        "webpack-node-externals": "^1.6.0"
